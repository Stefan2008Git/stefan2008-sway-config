{
  "layer": "top",
  "position": "top",
  "height": 32,
  "spacing": 4,
  "margin-top": 8,
  "margin-left": 10,
  "margin-right": 10,

  // Modules
  "modules-left": [
    "custom/launcher",
    "sway/workspaces",
    "mpris",
    "sway/mode"
  ],
  "modules-center": [
    "network"
  ],
  "modules-right": [
    "custom/notification",
    "cpu",
    "memory",
    "backlight",
    "pulseaudio",
    "clock",
    "bluetooth",
    "battery",
    "battery#bat1",
    "tray",
    "custom/power"
  ],

  "custom/launcher": {
        "format":"<span size='x-large'> Ôå¢ </span>",
        "on-click": "exec nwg-drawer",
        "tooltip": false
  },
  "tray": {
    "spacing": 10
  },
  "clock": {
    "format": "{:%H:%M}",
    "format-alt": "{:%d/%m/%Y}",
    "timezone": "Europe/Vienna",
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    "calendar": {
      "mode": "month",
      "format": {
        "months": "<span color='#cecece'><b>{}</b></span>",
        "days": "<span color='#dadada'><b>{}</b></span>",
        "weekdays": "<span color='#cecece'><b>{}</b></span>",
        "today": "<span color='#ffffff'><b><u>{}</u></b></span>"
      }
    }
  },
  "battery": {
    "bat": "BAT0",
    "states": {
      "good": 95,
      "warning": 30,
      "critical": 15
    },
    "format": "{icon} {capacity}%",
    "format-full": "{icon} {capacity}%",
    "format-charging": "Ôá¶ {capacity}%",
    "format-plugged": "Ôá¶ {capacity}%",
    "format-icons": [
      "ÔâÑ ",
      "ÔâÉ ",
      "ÔâÇ ",
      "ÔâÅ ",
      "ÔâÄ "
    ],
    "tooltip-format": "{time}",
    "interval": 1,
    "on-click": "control -p"
  },
  "battery#bat1": {
    "bat": "BAT1",
    "states": {
      "good": 95,
      "warning": 30,
      "critical": 15
    },
    "format": "{icon} {capacity}%",
    "format-full": "{icon} {capacity}%",
    "format-charging": "Ôá¶ {capacity}%",
    "format-plugged": "Ôá¶ {capacity}%",
    "format-icons": [
      "ÔâÑ ",
      "ÔâÉ ",
      "ÔâÇ ",
      "ÔâÅ ",
      "ÔâÄ "
    ],
    "tooltip-format": "{time}",
    "interval": 1,
    "on-click": "control -p"
  },
  "battery#bat2": {
    "bat": "BAT2"
  },
  "network": {
        "interval": 5,
        "format-wifi": " Ôá´  {essid} ({signalStrength}%)", // Icon: wifi
        "format-ethernet": "ÔÉ¨  {ifname}: {ipaddr}/{cidr}", // Icon: ethernet
        "format-disconnected": "‚ö†  Disconnected",
        "tooltip-format": "{ifname}: {ipaddr}",
        "on-click": "xfce4-terminal -e 'nmtui'",
  },
  "cpu": {
        "interval": 5,
        "format": "Ôíº {usage:>3}% ",
        "states": {
            "warning": 70,
            "critical": 90,
        },
        "on-click": "xfce4-terminal -e htop"
  },
  "memory": {
        "interval": 5,
        "format": "ÓøÖ {:>3}% ",
        "on-click": "xfce4-terminal -e htop",
        "states": {
            "warning": 70,
            "critical": 90
        }
  },
  "backlight": {
        "format": "{icon} {percent}% ",
        "format-icons": ["Û∞Ñ∞ ", "Û∞™û ", "Û∞™ü ", "Û∞™† ", "Û∞™° ", "Û∞™¢ ", "Û∞™£ ", "Û∞™§ ", "Û∞™• "],
        "tooltip-format": "Backlight (Scroll): {percent:}%",
        "on-scroll-down": "brightnessctl -c backlight set 5%-",
        "on-scroll-up": "brightnessctl -c backlight set +5%"
  },
  "pulseaudio": {
        // "scroll-step": 1, // %, can be a float
        "format": "{volume}% {icon} {format_source}",
        "format-bluetooth": "{volume}% {icon}Ôäî {format_source}",
        "format-bluetooth-muted": "Ôö© Muted {icon}Ôäî {format_source}",
        "format-muted": "Ôö© Muted {format_source}",
        "format-source": "{volume}% ÔÑ∞",
        "format-source-muted": "ÔÑ±",
        "format-icons": {
            "headphone": " ÔÄ• ",
            "hands-free": " Ôñê ",
            "headset": " Ôñê ",
            "phone": " ÔÇï ",
            "portable": " ÔÇï ",
            "car": " ÔÜπ ",
            "default": [" ÔÄ¶ ", " ÔÄß ", " ÔÄ® "]
        },
        "on-click": "pavucontrol"
  },
  "bluetooth": {
    "format": "Ôäî On",
    "format-disabled": "Û∞Ç≤ Off",
    "format-connected": "Û∞Ç± Connected",
    "on-click": "blueman-manager"
  },
  "tray": {
        "icon-size": 16,
        "spacing":10
  },
  "custom/power": {
        "format":" ‚èª  ",
        "on-click": "exec ~/.config/sway/scripts/power_Menu.py",
        "tooltip": false
  },
  "custom/notification": {
      "tooltip": false,
      "format": "{icon}",
      "format-icons": {
      "notification": "ÔÉ≥ <span foreground='#f38ba8'><sup>ÔëÑ </sup></span>",
      "none": "ÔÉ≥ ",
      "dnd-notification": "Ôá∂  <span foreground='#f38ba8'><sup>ÔëÑ </sup></span>",
      "dnd-none": "Ôá∂ "
    },
      "return-type": "json",
      "exec-if": "which swaync-client",
      "exec": "swaync-client -swb",
      "on-click": "swaync-client -t -sw",
      "on-click-right": "swaync-client -d -sw",
      "escape": true
  },
  "mpris": {
	  "format": " Playing: {player_icon} {dynamic}",
	  "format-paused": " Paused: {status_icon} <i>{dynamic}</i>",
	  "player-icons": {
		  "default": "‚ñ∂",
		  "mpv": "üéµ"
	},
	"status-icons": {
		"paused": "‚è∏"
	},
	  "ignored-players": ["firefox"]
  },
}
